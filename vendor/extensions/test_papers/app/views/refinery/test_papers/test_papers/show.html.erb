<% content_for :body_content_title do %>
  <%= @test_paper.score %>
<% end %>

<% content_for :body do %>
  <section>
    <h1>Wine</h1>
    <p>
      <%=raw @test_paper.wine_id %>
    </p>
  </section>
  <section>
    <h1>User</h1>
    <p>
      <%=raw @test_paper.user_id %>
    </p>
  </section>
  <section>
    <h1>Group</h1>
    <p>
      <%=raw @test_paper.group_id %>
    </p>
  </section>
  <section>
    <h1>Score</h1>
    <p>
      <%=raw @test_paper.score %>
    </p>
  </section>
  <section>
    <h1>Drink Begin At</h1>
    <p>
      <%=raw @test_paper.drink_begin_at %>
    </p>
  </section>
  <section>
    <h1>Drink End At</h1>
    <p>
      <%=raw @test_paper.drink_end_at %>
    </p>
  </section>
  <section>
    <h1>Note</h1>
    <p>
      <%=raw @test_paper.note %>
    </p>
  </section>
<% end %>

<% content_for :side_body do %>
  <aside>
    <h2><%= t('.other') %></h2>
    <ul id="test_papers">
      <% @test_papers.each do |test_paper| %>
        <li>
          <%= link_to test_paper.score, refinery.test_papers_test_paper_path(test_paper) %>
        </li>
      <% end %>
    </ul>
  </aside>
<% end %>

<%= render '/refinery/content_page' %>
